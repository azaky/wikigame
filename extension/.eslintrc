{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-undef": "off",
        "no-lonely-if": "off",
        "no-use-before-define": "off",
        "no-alert": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "no-loop-func": "off"
    }
}